              <DbSource ConnectionRef="dbSchoolAppConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT 
                         StudentRegistration.StudentID, StudentRegistration.FirstName + '  ' + ISNULL(StudentRegistration.LastName, '') AS FullName, StudentRegistration.FatherName, 
                         StudentRegistration.SMSMobileNo, StudentRegistration.EnrollmentNo, StudentRegistration.Gender, FeesCollectionMaster.ClassID, 
                         ClassSetup.ClassDescription AS Class, FeesCollectionMaster.ReceiptNo, FeesCollectionMaster.ReceiptDate, FeesCollectionMaster.Mode, 
                         FeesCollectionMaster.BankName, FeesCollectionMaster.BranchName, FeesCollectionMaster.ChequeNo, FeesCollectionMaster.ChequeDate, 
                         FeesCollectionMaster.TermTotal, FeesCollectionMaster.FineAmount, FeesCollectionMaster.OtherAmount, FeesCollectionMaster.OtherHead, 
                         FeesCollectionMaster.DiscountAmount, FeesCollectionMaster.PaidAmount, FeeTerm.FeeTermType AS Term, FeesCollectionDetail.Amount AS HeadAmount, 
                         FeesCollectionDetail.FeesHeadID, FeesHead.FeesHeadName
FROM            FeesCollectionMaster INNER JOIN
                         StudentRegistration ON FeesCollectionMaster.StudentID = StudentRegistration.StudentID INNER JOIN
                         Class ON FeesCollectionMaster.ClassID = Class.ClassID INNER JOIN
                         FeesCollectionDetail ON FeesCollectionMaster.ReceiptNo = FeesCollectionDetail.ReceiptNo AND FeesCollectionMaster.CompID = FeesCollectionDetail.CompID AND 
                         FeesCollectionMaster.BranchId = FeesCollectionDetail.BranchId LEFT OUTER JOIN
                         FeesHead ON FeesCollectionDetail.FeesHeadID = FeesHead.FeesHeadID LEFT OUTER JOIN
                         ClassSetup INNER JOIN
                         StudentSession ON ClassSetup.ClassSetupID = StudentSession.ClassSetupID AND ClassSetup.ClassID = StudentSession.ClassID ON 
                         FeesCollectionMaster.StudentID = StudentSession.StudentID AND FeesCollectionMaster.SessionID = StudentSession.SessionID LEFT OUTER JOIN
                         FeeTerm ON FeesCollectionMaster.TermIds = FeeTerm.FeeTermID AND FeesCollectionMaster.SessionID = FeeTerm.SessionId</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
