        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="FeesCollectionHeadTableAdapter" GeneratorDataComponentClassName="FeesCollectionHeadTableAdapter" Name="FeesCollectionHead" UserDataComponentName="FeesCollectionHeadTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="dbSchoolAppConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT ClassSetup.ClassDescription AS Class, FeeTerm.FeeTermType AS Term, FeesHead.FeesHeadName AS Head, 
                         StudentRegistration.FirstName + ' ' + ISNULL(StudentRegistration.LastName, '') AS Student, SUM(FeesCollectionDetail.Amount) AS Amount
FROM            FeesHead INNER JOIN
                         FeesCollectionDetail ON FeesHead.FeesHeadID = FeesCollectionDetail.FeesHeadID INNER JOIN
                         FeesCollectionMaster ON FeesCollectionDetail.ReceiptNo = FeesCollectionMaster.ReceiptNo AND 
                         FeesCollectionDetail.SessionID = FeesCollectionMaster.SessionID INNER JOIN
                         FeeTerm ON FeesCollectionDetail.FeeTermID = FeeTerm.FeeTermID INNER JOIN
                         ClassSetup ON FeesCollectionMaster.ClassID = ClassSetup.ClassID INNER JOIN
                         StudentRegistration ON FeesCollectionMaster.StudentID = StudentRegistration.StudentID
GROUP BY ClassSetup.ClassDescription, FeeTerm.FeeTermType, FeesHead.FeesHeadName, StudentRegistration.FirstName + ' ' + ISNULL(StudentRegistration.LastName, '')
ORDER BY Class, Term, Head, Student</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
