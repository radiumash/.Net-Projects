                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        StudentRegistration.FirstName + '  ' + ISNULL(StudentRegistration.LastName, '') AS FullName, StudentRegistration.FatherName, StudentRegistration.SMSMobileNo, 
                         StudentRegistration.EnrollmentNo, StudentRegistration.Gender, ClassSetup.ClassDescription AS Class, FeesCollectionMaster.ReceiptNo, 
                         FeesCollectionMaster.ReceiptDate, FeesCollectionMaster.Mode, FeesCollectionMaster.BankName, FeesCollectionMaster.BranchName, 
                         FeesCollectionMaster.ChequeNo, FeesCollectionMaster.ChequeDate, FeesCollectionMaster.TermTotal, FeesCollectionMaster.FineAmount, 
                         FeesCollectionMaster.OtherAmount, FeesCollectionMaster.OtherHead, FeesCollectionMaster.DiscountAmount, FeesCollectionMaster.PaidAmount, '' AS Term, 
                         StudentRegistration.LocalAddress, StudentRegistration.ParmanentAddress, UserLogin.FullName AS UserFullName, UserLogin.UserID, UserLogin.UserName, 
                         FeeTerm.FeeTermName
FROM            FeeTerm INNER JOIN
                         FeesCollectionMaster INNER JOIN
                         StudentRegistration ON FeesCollectionMaster.StudentID = StudentRegistration.StudentID ON FeeTerm.FeeTermID = FeesCollectionMaster.TermIds LEFT OUTER JOIN
                         UserLogin INNER JOIN
                         ClassSetup INNER JOIN
                         StudentSession ON ClassSetup.ClassSetupID = StudentSession.ClassSetupID AND ClassSetup.ClassID = StudentSession.ClassID ON 
                         UserLogin.UserID = ClassSetup.UIDAdd ON FeesCollectionMaster.StudentID = StudentSession.StudentID AND 
                         FeesCollectionMaster.SessionID = StudentSession.SessionID</CommandText>
