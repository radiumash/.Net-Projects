                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Class.ClassName, Section.SectionName, StudentSession.RollNo, '' AS Rank, '' AS Rank1, '' AS Rank2, '' AS Rank3, '' AS Rank4, '' AS OverallRank, StudentRegistration.EnrollmentNo, 
                         StudentRegistration.FirstName + ' ' + ISNULL(StudentRegistration.LastName, '') AS StudentName, StudentRegistration.DateOfBirth, ExamRemarkEntry.TeacherRemark, ExamRemarkEntry.PrincipalRemark, 
                         ExamRemarkEntry.Height, ExamRemarkEntry.Weight, ExamRemarkEntry.AttendanceTotal, ExamRemarkEntry.AttendancePresent, StudentRegistration.StudentID, StudentSession.ClassSetupID, StudentSession.ClassID, 
                         StudentRegistration.FatherName, Session.SessionName
FROM            StudentRegistration INNER JOIN
                         StudentSession ON StudentRegistration.StudentID = StudentSession.StudentID AND StudentRegistration.CompID = StudentSession.CompID AND StudentRegistration.BranchId = StudentSession.BranchId INNER JOIN
                         ClassSetup ON StudentSession.ClassSetupID = ClassSetup.ClassSetupID AND StudentSession.ClassID = ClassSetup.ClassID AND StudentSession.BranchId = ClassSetup.BranchId AND 
                         StudentSession.CompID = ClassSetup.CompID INNER JOIN
                         Class ON ClassSetup.ClassID = Class.ClassID AND ClassSetup.BranchId = Class.BranchId AND ClassSetup.CompID = Class.CompID INNER JOIN
                         Section ON ClassSetup.SectionID = Section.SectionID AND ClassSetup.CompID = Section.CompID AND ClassSetup.BranchId = Section.BranchId INNER JOIN
                         Session ON StudentSession.SessionID = Session.SessionId LEFT OUTER JOIN
                         ExamRemarkEntry ON StudentSession.StudentID = ExamRemarkEntry.StudentID AND StudentSession.SessionID = ExamRemarkEntry.SessionID AND StudentSession.ClassID = ExamRemarkEntry.ClassID AND 
                         StudentSession.CompID = ExamRemarkEntry.CompID AND StudentSession.BranchId = ExamRemarkEntry.BranchID</CommandText>
