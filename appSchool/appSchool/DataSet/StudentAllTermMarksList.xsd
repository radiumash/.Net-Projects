        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="StudentRegistrationTableAdapter" GeneratorDataComponentClassName="StudentRegistrationTableAdapter" Name="StudentRegistration" UserDataComponentName="StudentRegistrationTableAdapter">
