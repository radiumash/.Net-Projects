                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        StudentSession.RollNo, ExamResult.ObtainMarks1, StudentRegistration.FirstName + ' ' + ISNULL(StudentRegistration.LastName, '') AS StudentName, 
                         Class.ClassName AS Class, Section.SectionName AS Section, ExamResult.SubjectIDL2, StudentRegistration.StudentID, ExamRemarkEntry.ExamID, 
                         ExamRemarkEntry.AttendanceTotal, ExamRemarkEntry.AttendancePresent
FROM            ExamResult INNER JOIN
                         StudentRegistration ON ExamResult.StudentID = StudentRegistration.StudentID AND ExamResult.BranchID = StudentRegistration.BranchId AND 
                         ExamResult.CompID = StudentRegistration.CompID INNER JOIN
                         StudentSession ON ExamResult.StudentID = StudentSession.StudentID AND ExamResult.BranchID = StudentSession.BranchId AND 
                         ExamResult.CompID = StudentSession.CompID AND ExamResult.SessionID = StudentSession.SessionID INNER JOIN
                         ExamRemarkEntry ON StudentSession.StudentID = ExamRemarkEntry.StudentID INNER JOIN
                         Class ON StudentSession.ClassID = Class.ClassID INNER JOIN
                         ClassSetup ON StudentSession.ClassSetupID = ClassSetup.ClassSetupID AND StudentSession.ClassID = ClassSetup.ClassID INNER JOIN
                         Section ON ClassSetup.SectionID = Section.SectionID
WHERE        (ExamResult.ClassID = 3) AND (StudentSession.ClassSetupID = 3) AND (ExamResult.BranchID = 1) AND (ExamResult.CompID = 1) AND 
                         (StudentRegistration.TCGiven = 0) AND (ExamResult.SessionID = 3) AND (ExamRemarkEntry.ExamID = 1)</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
