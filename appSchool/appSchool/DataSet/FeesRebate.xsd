        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="FeesRebateTableAdapter" GeneratorDataComponentClassName="FeesRebateTableAdapter" Name="FeesRebate" UserDataComponentName="FeesRebateTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="dbSchoolAppConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        dbo.StudentRegistration.FirstName + '  ' + ISNULL(dbo.StudentRegistration.LastName, '') AS FullName, dbo.StudentRegistration.FatherName, 
                         dbo.StudentRegistration.SMSMobileNo, dbo.StudentRegistration.EnrollmentNo, dbo.StudentRegistration.Gender, dbo.ClassSetup.ClassDescription AS Class, 
                         dbo.StudentFeesMaster.PaidAmount, dbo.vFeesStructure.Amount , dbo.FeeTerm.FeeTermType as Term
FROM            dbo.FeeTerm RIGHT OUTER JOIN
                         dbo.StudentFeesMaster INNER JOIN
                         dbo.StudentRegistration ON dbo.StudentFeesMaster.StudentID = dbo.StudentRegistration.StudentID INNER JOIN
                         dbo.vFeesStructure ON dbo.StudentFeesMaster.StudentClassId = dbo.vFeesStructure.ClassId AND dbo.StudentFeesMaster.TermID = dbo.vFeesStructure.TermId AND 
                         dbo.vFeesStructure.Amount &gt; dbo.StudentFeesMaster.PaidAmount ON dbo.FeeTerm.FeeTermID = dbo.StudentFeesMaster.TermID AND 
                         dbo.FeeTerm.SessionId = dbo.StudentFeesMaster.SessionID LEFT OUTER JOIN
                         dbo.ClassSetup INNER JOIN
                         dbo.StudentSession ON dbo.ClassSetup.ClassSetupID = dbo.StudentSession.ClassSetupID ON 
                         dbo.StudentFeesMaster.StudentID = dbo.StudentSession.StudentID AND dbo.StudentFeesMaster.SessionID = dbo.StudentSession.SessionID
WHERE        (dbo.StudentFeesMaster.PaidFlag = 1)</CommandText>
