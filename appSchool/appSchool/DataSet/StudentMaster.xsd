                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        StudentSession.RollNo, StudentRegistration.FirstName + ' ' + ISNULL(StudentRegistration.LastName, '') AS StudentName, StudentRegistration.FatherName AS Father, StudentRegistration.DateOfBirth, 
                         StudentRegistration.SMSMobileNo AS MobileNo, StudentRegistration.EnrollmentNo, StudentRegistration.StudentID, StudentRegistration.Gender, StudentRegistration.ParmanentAddress, StudentRegistration.LocalAddress, 
                         StudentRegistration.Religion, StudentRegistration.MotherName, StudentRegistration.EnrollmentDate, StudentRegistration.AppImageName, StudentRegistration.AppImage, '' AS Class, '' AS SectionName, 
                         StudentPreviousDetail.PreviousSchoolName, StudentPreviousDetail.DateOfLeaving, StudentRegistration.TCNumber, StudentRegistration.TCDate, StudentRegistration.PromotionType, StudentRegistration.Stream, 
                         StudentRegistration.TCGiven, ' ' AS ExamCode
FROM            StudentRegistration WITH (NOLOCK) INNER JOIN
                         StudentSession WITH (NOLOCK) ON StudentRegistration.StudentID = StudentSession.StudentID LEFT OUTER JOIN
                         StudentPreviousDetail ON StudentRegistration.StudentID = StudentPreviousDetail.StudentID</CommandText>
