                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Class.ClassName, Section.SectionName, StudentSession.RollNo, '' AS Rank, StudentRegistration.EnrollmentNo, 
                         StudentRegistration.FirstName + ' ' + ISNULL(StudentRegistration.LastName, '') AS StudentName, StudentRegistration.FatherName, StudentRegistration.DateOfBirth, 
                         ExamRemarkEntry.TeacherRemark AS TeacherRemark2, ExamRemarkEntry.PrincipalRemark, ExamRemarkEntry.Height, ExamRemarkEntry.Weight, 
                         ExamRemarkEntry.AttendanceTotal AS AttendanceTotalTerm2, ExamRemarkEntry.AttendancePresent AS AttendancePresentTerm2, 
                         StudentRegistration.StudentID
FROM            StudentRegistration INNER JOIN
                         StudentSession ON StudentRegistration.StudentID = StudentSession.StudentID AND StudentRegistration.CompID = StudentSession.CompID AND 
                         StudentRegistration.BranchID = StudentSession.BranchID INNER JOIN
                         ClassSetup ON StudentSession.ClassSetupID = ClassSetup.ClassSetupID AND StudentSession.ClassID = ClassSetup.ClassID AND 
                         StudentSession.BranchID = ClassSetup.BranchID AND StudentSession.CompID = ClassSetup.CompID INNER JOIN
                         Class ON ClassSetup.ClassID = Class.ClassID AND ClassSetup.BranchID = Class.BranchID AND ClassSetup.CompID = Class.CompID INNER JOIN
                         Section ON ClassSetup.SectionID = Section.SectionID AND ClassSetup.CompID = Section.CompID AND ClassSetup.BranchID = Section.BranchID LEFT OUTER JOIN
                         ExamRemarkEntry ON StudentSession.StudentID = ExamRemarkEntry.StudentID AND StudentSession.SessionID = ExamRemarkEntry.SessionID AND 
                         StudentSession.ClassID = ExamRemarkEntry.ClassID AND StudentSession.CompID = ExamRemarkEntry.CompID AND 
                         StudentSession.BranchID = ExamRemarkEntry.BranchID</CommandText>
